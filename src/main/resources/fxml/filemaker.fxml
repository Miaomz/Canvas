<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="120.0"
          prefWidth="200.0" xmlns="http://javafx.com/javafx/9"
          fx:controller="casual.canvas.presentation.FileController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="96.0" minWidth="10.0" prefWidth="76.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="127.0" minWidth="10.0" prefWidth="124.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="107.0" minHeight="10.0" prefHeight="73.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="71.0" minHeight="10.0" prefHeight="47.0" vgrow="SOMETIMES" />
  </rowConstraints>
      <TextField fx:id="inputField" GridPane.columnIndex="1" text="unnamed">
         <GridPane.margin>
            <Insets right="10.0" />
         </GridPane.margin>
      </TextField>
      <Label alignment="CENTER" text="File Name:">
         <GridPane.margin>
            <Insets left="5.0" />
         </GridPane.margin>
      </Label>
      <Button mnemonicParsing="false" text="Cancel" GridPane.rowIndex="1" onAction="#cancel">
         <GridPane.margin>
            <Insets left="15.0" />
         </GridPane.margin>
      </Button>
      <Button mnemonicParsing="false" text="Confirm" GridPane.columnIndex="1" GridPane.rowIndex="1" onAction="#confirm">
         <GridPane.margin>
            <Insets left="45.0" />
         </GridPane.margin>
      </Button>
</GridPane>
